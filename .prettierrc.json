{
  "printWidth": 130,
  "tabWidth": 2,
  "useTabs": true,
  "semi": true,
  "singleQuote": true,
  "quoteProps": "as-needed",
  "jsxSingleQuote": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "always",
  "endOfLine": "lf",
  "singleAttributePerLine": true,
  "multilineArraysWrapThreshold": 0,
  "plugins": [
    "./node_modules/prettier-plugin-multiline-arrays"
  ]
}